{
    "name": "protoc-gen-typescript",
    "version": "0.0.1",
    "description": "Protocol Buffers Generator for TypeScript",
    "main": "build/plugin.js",
    "scripts": {
        "build": "node --max_old_space_size=2500 ./node_modules/typescript/bin/tsc --project ./tsconfig.json",
        "watch": "node --max_old_space_size=2500 ./node_modules/typescript/bin/tsc --project ./tsconfig.json --watch"
    },
    "dependencies": {
        "google-protobuf": "^3.17.0"
    },
    "devDependencies": {
        "@types/google-protobuf": "^3.15.2",
        "@types/node": "^15.0.2",
        "typescript": "^4.2.3"
    }
}
